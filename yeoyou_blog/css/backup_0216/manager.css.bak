.trading-area .trading-box .search .detail-search .btn-search{position:absolute;right:32px;top:50%;width:93px;height:95px;margin-top:-48px;border:0;font-family:NanumGothicWeb;font-size:14px;color:#fff;font-weight:600;background:url(/img/btn_sprite.png) no-repeat -392px -119px;text-indent:0}

.trading-area .trading-box .trade-board{margin-top:45px;background:url(/img/bg_trading_box.png) no-repeat -1120px 100%}
.trading-area .trading-box .trade-board .in{padding:0 1px 1px 0;background:url(/img/bg_trading_box.png) no-repeat 0 0}
.trading-area .trading-box .trade-board .board{width:100%;font-family:NanumGothicWeb}
.trading-area .trading-box .trade-board .board th{height:43px;border-width:2px 1px 1px;border-style:solid;border-color:#4f657f #e7e7e7;font-size:13px;color:#959595;font-weight:500;background:#f4f5f6}
.trading-area .trading-box .trade-board .board th span{display:inline-block;position:relative;padding-right:10px;cursor:pointer}
.trading-area .trading-box .trade-board .board th span:after{position:absolute;right:0;top:50%;display:block;content:"";width:7px;height:6px;margin-top:-3px;background:url(/img/ico_detail_search.gif) no-repeat 0 -6px}{background:url(/img/ico_detail_search.gif) no-repeat 100% 50%}
.trading-area .trading-box .trade-board .board th span.on:after{background-position:0 0}
.trading-area .trading-box .trade-board .board .clm th{border-top-width:1px;border-top-color:#e7e7e7}
.trading-area .trading-box .trade-board .board .clm2{border-bottom-color:#e7e7e7}
.trading-area .trading-box .trade-board .board td{padding:10px 0;border-width:0 1px 1px;border-style:solid;border-color:#e7e7e7;color:#555;text-align:center;line-height:14px}
.trading-area .trading-box .trade-board .board .subject{display:block;color:#a5a4a4}
.trading-area .trading-box .trade-board .board .subject a{color:#a5a4a4}
.trading-area .trading-box .paging{margin-top:50px}