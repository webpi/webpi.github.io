.box-login .box-conts .certify .method li .decorate img{padding-bottom:10px}
.box-login .box-conts .certify-noti{margin-top:5px;font-size:13px;color:#555}

/* 메인 */
.section-index{width:1000px;margin:0 auto}
.section-index .contents-area{border-width:0 1px 1px;border-style:solid;border-color:#cfd2d9}
.section-index .box-conts{float:right;width:330px}

/* 로그인전 */
.section-index .box-conts .login{height:137px;padding:32px 16px 0;border-bottom:1px solid #cfd2d9}
.box-conts .login .iuser{position:relative;padding-right:92px}
.box-conts .login .iuser .id-login,
.box-conts .login .iuser .pw-login{display:block;width:162px;height:35px;padding:2px 0 0 40px;background:url(/assets/img/bg_login_input.png) no-repeat}
.box-conts .login .iuser .id-login{background-position:0 0}
.box-conts .login .iuser .pw-login{margin-top:5px;background-position:-202px 0}
