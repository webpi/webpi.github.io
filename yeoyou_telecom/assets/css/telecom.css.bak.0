.section-index .box-conts .account .my-phone-number .btn-phone-reset{position:absolute;top:50%;right:11px;width:60px;height:20px;margin-top:-10px;background-position:0 0;font-family:'돋움',Dotum;font-size:11px;line-height:20px;color:#fff}

.section-index .box-conts .account .account-aside{margin-top:17px;padding:0 10px}
