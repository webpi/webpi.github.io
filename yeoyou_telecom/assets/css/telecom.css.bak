@charset "utf-8";

/* common */
.box-header{position:relative;height:118px;border-bottom:1px solid #cdcdcd}
.box-header.bg{background:url(/assets/img/bg_box_header.png) no-repeat 100% 100%}
.box-header.bg2{background:url(/assets/img/bg_box_header2.png) no-repeat 100% 100%}
.box-header .dsc{position:absolute;bottom:10px;left:0;font-size:14px;color:#707070}

#contents-area .pic{margin-top:40px}

/* 메인 */
.section-index{width:1000px;margin:0 auto}
.section-index .contents-area{border-width:0 1px 1px;border-style:solid;border-color:#cfd2d9}
.section-index .box-conts{float:right;width:330px}

/* 로그인전 */
.section-index .box-conts .login{height:137px;padding:32px 16px 0;border-bottom:1px solid #cfd2d9}
.section-index .box-conts .login .iuser{position:relative;padding-right:92px}
.section-index .box-conts .login .iuser .id-login,
.section-index .box-conts .login .iuser .pw-login{display:block;width:162px;height:35px;padding:2px 0 0 40px;background:url(/assets/img/bg_login_input.gif) no-repeat}
.section-index .box-conts .login .iuser .id-login{background-position:0 0}
.section-index .box-conts .login .iuser .pw-login{margin-top:5px;background-position:-202px 0}
.section-index .box-conts .login .iuser .id-login input,
.section-index .box-conts .login .iuser .pw-login input{display:block;height:33px;padding:0 5px;border:0;line-height:33px}
.section-index .box-conts .login .iuser .placeholder label{padding-left:45px;color:#aaa;line-height:37px}
.section-index .box-conts .login .iuser .btn-login{position:absolute;right:0;top:0;width:92px;height:79px;font-size:14px;color:#fb2f57;background-position:-162px 0;line-height:79px}
.section-index .box-conts .login .iuser .btn-login:hover{color:#fff;background-position:-162px -84px}

.section-index .box-conts .login .login-aside{margin-top:20px}
.section-index .box-conts .login .login-aside li{float:right}
.section-index .box-conts .login .login-aside li:first-child{float:left}
.section-index .box-conts .login .login-aside li a{color:#333}

/* 로그인후 */
.section-index .box-conts .account{height:150px;padding:19px 16px 0;border-bottom:1px solid #cfd2d9}
.section-index .box-conts .account .user-info{padding:0 10px 10px;border-bottom:1px solid #cfd2d9}
.section-index .box-conts .account .user-info .name{float:left;font-size:14px}
.section-index .box-conts .account .user-info .name .ico{margin-right:10px}
.section-index .box-conts .account .user-info .donut-pay{float:right;padding-top:1px;font-size:13px;color:#777}
.section-index .box-conts .account .user-info .donut-pay strong{color:#333}
.section-index .box-conts .account .user-info .donut-pay strong i{margin-right:6px}

.section-index .box-conts .account .my-phone-number{position:relative;height:37px;margin-top:17px;font-size:13px;color:#777;background:url(/assets/img/bg_myphone_number.png) no-repeat 100% -37px}
.section-index .box-conts .account .my-phone-number .in{height:37px;padding-left:20px;background:url(/assets/img/bg_myphone_number.png) no-repeat 0 0;line-height:37px}
.section-index .box-conts .account .my-phone-number strong{color:#333}
.section-index .box-conts .account .my-phone-number .btn-phone-reset{position:absolute;right:11px;top:50%;width:60px;height:20px;margin-top:-10px;font-family:"돋움",Dotum;font-size:11px;color:#fff;background-position:0 0;line-height:20px}

.section-index .box-conts .account .account-aside{margin-top:17px;padding:0 10px}
.section-index .box-conts .account .account-aside li{float:right}
.section-index .box-conts .account .account-aside li:first-child{float:left;padding-top:5px;font-size:13px}
.section-index .box-conts .account .account-aside li:first-child a{color:#777}
.section-index .box-conts .account .account-aside li:first-child .ico-lock{margin-right:10px}
.section-index .box-conts .account .account-aside .btn-logout{width:92px;height:30px;font-size:13px;color:#fb2f57;background-position:-65px 0;line-height:30px}
.section-index .box-conts .account .account-aside .btn-logout:hover{color:#fff;background-position:-65px -35px}

/* section-index */
.section-index .box-conts .lnb{position:relative;height:275px;padding:20px 20px 0;border-bottom:1px solid #cfd2d9}
.section-index .box-conts .lnb .tit-h2{font-size:12px}
