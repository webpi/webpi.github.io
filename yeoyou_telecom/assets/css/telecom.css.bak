.section-index .box-conts .quick-service{position:relative}
.section-index .box-conts .quick-service .tit-h2{height:30px;padding:12px 0 0 20px;border-bottom:1px solid #cfd2d9}
.section-index .box-conts .quick-service .nav{padding:12px 0 0 19px}
.section-index .box-conts .quick-service .nav li{float:left;width:50%;padding:10px 0}
.section-index .box-conts .quick-service .btn-customer-service{position:absolute;right:17px;top:14px}

.section-index .aside-area .column-lft{float:left;width:658px;height:50px;padding:4px 0;border-bottom:1px solid #cfd2d9}
