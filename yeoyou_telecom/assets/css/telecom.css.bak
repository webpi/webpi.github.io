.box-guide .possible-hp{margin-top:65px}
.box-guide .possible-hp ul{margin-top:25px}
.box-guide .possible-hp p{margin-top:20px;line-height:22px}
.box-guide .button-group{margin-top:65px}

/* 알뜰폰 / 가입 */
.box-join .conts{position:relative;margin-top:50px;padding-right:250px}
.box-join .conts .tit-h4{padding-bottom:6px;font-size:18px;color:#555}
