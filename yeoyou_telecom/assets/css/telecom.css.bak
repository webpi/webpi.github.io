.section-index .aside-area .column-lft .notice .tit-h2{position:absolute;left:20px;top:17px}
.section-index .aside-area .column-lft .notice li{line-height:50px}
.section-index .aside-area .column-lft .notice li a{font-size:13px;color:#666}
.section-index .aside-area .column-lft .notice .btn-notice-more{position:absolute;top:50%;right:10px;margin-top:-7px}

.section-index .aside-area .column-rgt{float:right;width:330px;height:54px;padding-top:4px;border-bottom:1px solid #cfd2d9}
.section-index .aside-area .column-rgt .btn-request-search{display:block;height:50px;padding-left:50px;font-size:14px;color:#fff;background:url(/assets/img/btn_request_search.png) no-repeat 100% 0;line-height:50px}

/* YEOYOU */
.box-intro .yeoyou-intro{margin-top:47px}
.box-intro .yeoyou-intro li{float:left;width:50%;margin-top:33px}
.box-intro .button-group{margin-top:70px}

.box-brand .brand-identity{margin-top:40px}
.box-brand .brand-identity .t{margin-top:45px}
.box-brand .brand-identity .dsc{margin-top:35px;font-size:14px;line-height:24px}

.box-brand .logosign{margin-top:75px}
