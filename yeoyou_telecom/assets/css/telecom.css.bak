.section-index .box-conts .lnb .nav{padding-top:10px}
.section-index .box-conts .lnb .nav li{float:left;width:33.3%;margin-top:40px;text-align:center}
.section-index .box-conts .lnb .nav li a{display:inline-block;height:70px;background:url(/assets/img/btn_lnb.png) no-repeat;vertical-align:top}
.section-index .box-conts .lnb .nav .btn-lnb{width:47px;background-position:0 0}
.section-index .box-conts .lnb .nav .btn-lnb2{width:61px;background-position:-47px 0}
.section-index .box-conts .lnb .nav .btn-lnb3{width:84px;background-position:-108px 0}
.section-index .box-conts .lnb .nav .btn-lnb4{width:62px;background-position:-192px 0}
.section-index .box-conts .lnb .nav .btn-lnb5{width:46px;background-position:-254px 0}
.section-index .box-conts .lnb .nav .btn-lnb6{width:75px;background-position:-300px 0}

.section-index .box-conts .lnb .nav .btn-lnb:hover{background-position:0 -70px}
.section-index .box-conts .lnb .nav .btn-lnb2:hover{background-position:-47px -70px}
.section-index .box-conts .lnb .nav .btn-lnb3:hover{background-position:-108px -70px}
.section-index .box-conts .lnb .nav .btn-lnb4:hover{background-position:-192px -70px}
.section-index .box-conts .lnb .nav .btn-lnb5:hover{background-position:-254px -70px}
.section-index .box-conts .lnb .nav .btn-lnb6:hover{background-position:-300px -70px}

.section-index .box-aside{float:left;width:668px}
.section-index .box-aside .adv-main{height:465px;border-width:0 1px 1px 0;border-style:solid;border-color:#cfd2d9 #e4e4e4}
.section-index .box-aside .adv-main .slider-wrap{position:relative}
.section-index .box-aside .adv-main .slider-wrap .btn-slider-prev,
.section-index .box-aside .adv-main .slider-wrap .btn-slider-next{position:absolute;top:50%;cursor:pointer}
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-prev,
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-next{width:25px;height:44px;margin-top:-22px;background:url(/assets/img/btn_slider.png) no-repeat}
.section-index .box-aside .adv-main .slider-wrap.type1 .slider{overflow:hidden;width:667px;height:465px}
.section-index .box-aside .adv-main .slider-wrap.type1 .slider li{float:left}
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-prev{left:15px;background-position:0 0}
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-next{right:15px;background-position:-25px 0}
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-prev:hover{background-position:0 -44px}
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-next:hover{right:15px;background-position:-25px -44px}
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-prev a,
.section-index .box-aside .adv-main .slider-wrap.type1 .btn-slider-next a{display:block;height:44px;text-indent:-99999px}
.section-index .box-aside .adv-main .slider-wrap.type1 .carousel{position:absolute;bottom:25px;width:100%;font-size:0;line-height:0;text-align:center}
.section-index .box-aside .adv-main .slider-wrap.type1 .carousel a{display:inline-block;width:13px;height:13px;margin-left:5px;font-size:12px;line-height:normal;background:url(/assets/img/btn_type1_carousel.png) no-repeat 0 0;vertical-align:top}
.section-index .box-aside .adv-main .slider-wrap.type1 .carousel a:first-child{margin-left:0}
.section-index .box-aside .adv-main .slider-wrap.type1 .carousel a.active{background-position:0 -13px}

.section-index .box-aside .adv-sub{border-right:1px solid #cfd2d9}
.section-index .box-aside .adv-sub .adv-lft{float:left;padding-right:1px;background:url(/assets/img/bar_adv_sub.png) no-repeat 100% 0}
.section-index .box-aside .adv-sub .adv-rgt{float:right}
